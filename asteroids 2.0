class Locomotion { 
  float ypos, xpos;
  Locomotion (float x_ini, float y_ini) {
    xpos = x_ini;
    ypos = y_ini;
  }
  void update(){
    xpos = xpos - v_xpos;
    ypos = ypos - v_ypos;
  }
}

